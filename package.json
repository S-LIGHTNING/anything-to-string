  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "unminified-webpack-plugin": "^3.0.0",
    "webpack": "~5.101.0",
    "webpack-cli": "^6.0.0"
